    <style name="SwitchStyle" parent="Theme.AppCompat.Light">

        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">#00E8DC</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#9DC3ED</item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#9DC3ED</item>
    </style>
    <style name="DialogTheme">
        <item name="android:windowEnterAnimation">@anim/bottom_top</item>
        <item name="android:windowExitAnimation">@anim/top_bottom</item>
    </style>



    <style name="TextLabel" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">#99FFFFFF</item>
        <item name="android:textSize">15sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">#99FFFFFF</item>
        <item name="colorControlNormal">#99FFFFFF</item>
        <item name="colorControlActivated">#99FFFFFF</item>
    </style>


    <style name="TextLabel_boa" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/boa_txtsbold</item>
        <item name="android:textSize">15sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
